using System;

class Program
{
    static void Main()
    {
        // Definir las variables
        int x = 3;
        int y = 4;

        // Calcular la suma
        int suma = x + y;

        // Verificar si la suma es mayor o igual a 10 o menor o igual a 10
        if (suma > 10)
        {
            Console.WriteLine("La suma es mayor que 10: " + suma);
        }
        else
        {
            Console.WriteLine("La suma es menor o igual a 10: " + suma);
        }
    }
}
---
class Program {
        static void Main() {
            // Pedir al usuario que ingrese una nota
            Console.Write("Introduce una nota (entre 0 y 10): ");
            int nota = int.Parse(Console.ReadLine());

            // Comprobar si la nota está en el rango válido
            if (nota < 0 || nota > 10) {
                Console.WriteLine("Error: La nota debe estar entre 0 y 10.");
            } else {
                // Evaluar la nota y dar el mensaje correspondiente
                if (nota < 5) {
                    Console.WriteLine("Suspenso");
                } else if (nota >= 5 && nota < 6) {
                    Console.WriteLine("Aprobado");
                } else if (nota >= 6 && nota < 7) {
                    Console.WriteLine("Bien");
                } else if (nota >= 7 && nota < 9) {
                    Console.WriteLine("Notable");
                } else if (nota >= 9 && nota <= 10) {
                    Console.WriteLine("Sobresaliente");
                }
            }
        }
    }
---
